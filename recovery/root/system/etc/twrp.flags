# mount point       fstype    device                                                                flags
# Dynamic partition
/system              ext4      /dev/block/mapper/system               flags=slotselect
/system_image	     emmc      /dev/block/mapper/system               flags=slotselect;display="system";backup=1;flashimg=1
/system_ext_image    emmc      /dev/block/mapper/system_ext           flags=slotselect;display="system_ext";backup=1;flashimg=1
/vendor              ext4      /dev/block/mapper/vendor               flags=slotselect
/vendor_image	     emmc      /dev/block/mapper/vendor               flags=slotselect;display="Vendor";backup=1;flashimg=1
/product             ext4      /dev/block/mapper/product              flags=slotselect
/product_image	     emmc      /dev/block/mapper/product              flags=slotselect;display="product";backup=1;flashimg=1 

/tranfs             ext4      /dev/block/platform/bootdevice/by-name/tranfs
/protect_f          ext4      /dev/block/platform/bootdevice/by-name/protect1                       flags=display="Protect_f";backup=1
/protect_s          ext4      /dev/block/platform/bootdevice/by-name/protect2                       flags=display="Protect_s";backup=1
/nvcfg		        ext4      /dev/block/platform/bootdevice/by-name /nvcfg
/nvdata             ext4      /dev/block/platform/bootdevice/by-name/nvdata                         flags=display="Nvdata";backup=1;
/persist            ext4      /dev/block/platform/bootdevice/by-name/persist                        flags=display="Persist";backup=1
/persistent         emmc      /dev/block/platform/bootdevice/by-name/frp                            flags=display="Persistent";backup=1
/nvram              emmc      /dev/block/platform/bootdevice/by-name/nvram                          flags=display="Nvram";backup=1;flashimg=1
/proinfo            emmc      /dev/block/platform/bootdevice/by-name/proinfo          
/bootloader         emmc      /dev/block/platform/bootdevice/by-name/lk    
/logo	            emmc      /dev/block/platform/bootdevice/by-name/logo
/boot               emmc      /dev/block/platform/bootdevice/by-name/boot                            flags=slotselect;display="boot";backup=1;flashimg=1
/expdb              emmc      /dev/block/platform/bootdevice/by-name/expdb
/seccfg             emmc      /dev/block/platform/bootdevice/by-name/seccfg   
/tee                emmc      /dev/block/platform/bootdevice/by-name/tee         
/scp                emmc      /dev/block/platform/bootdevice/by-name/scp
/md1img             emmc      /dev/block/platform/bootdevice/by-name/md1img
/gz                 emmc      /dev/block/platform/bootdevice/by-name/gz
/spmfw              emmc      /dev/block/platform/bootdevice/by-name/spmfw     
/dtbo               emmc      /dev/block/platform/bootdevice/by-name/dtbo
/otp                emmc      /dev/block/platform/bootdevice/by-name/otp
/vbmeta             emmc      /dev/block/platform/bootdevice/by-name/vbmeta                                              
/vbmeta_system      emmc      /dev/block/platform/bootdevice/by-name/vbmeta_system            
/vbmeta_vendor      emmc      /dev/block/platform/bootdevice/by-name/vbmeta_vendor

# Removablee
/external_sd        Auto      /dev/block/mmcblk0p1         /dev/block/mmcblk0     
/usb-otg            vfat      /dev/block/sdd1                     flags=display="usb-otg";storage;wipeingui;removable;fsflags="utf8";